  <em:targetApplication>
    <Description>
      <em:id>{33cb9019-c295-46dd-be21-8c4936574bee}</em:id>
      <em:minVersion>1.6a1pre</em:minVersion>
      <em:maxVersion>1.6a1pre</em:maxVersion>
